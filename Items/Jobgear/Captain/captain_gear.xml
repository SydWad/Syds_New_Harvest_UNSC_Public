  <!-- Pilot Helmet -->
  <Item name="" identifier="nh_pilot_helmet" category="Equipment" tags="smallitem,clothing,nh_needs_battery" fireproof="false" cargocontaineridentifier="metalcrate" description="" scale="0.35" impactsoundtag="impact_metal_light">
    <PreferredContainer secondary="nh_dev_armor" minamount="1" maxamount="1" spawnprobability="1" />
    <Fabricate suitablefabricators="fabricator" requiredtime="20">
      <RequiredSkill identifier="helm" level="60" />
      <RequiredSkill identifier="weapons" level="30" />
      <RequiredItem identifier="thermalgoggles" />
      <RequiredItem identifier="titaniumaluminiumalloy" />
      <RequiredItem identifier="ballisticfiber" amount="2" />
    </Fabricate>
    <!-- 1 FPGA, 3 plastic, 2 aluminum 3 titanium -->
    <Deconstruct time="5">
      <Item identifier="fpgacircuit" />
      <Item identifier="plastic" amount="2" />
      <Item identifier="aluminum" />
      <Item identifier="titanium" amount="2" />
    </Deconstruct>
    <Sprite name="Captain Uniform" texture="%ModDir%/Items/Jobgear/Armor/headgears.png" sourcerect="100,200,100,100" depth="0.6" origin="0.5,0.5" />
    <Body radius="30" density="50" />
    <Wearable slots="Head" msg="ItemMsgPickUpSelect">
      <!-- Fake modifier to show speed change -->
      <damagemodifier afflictionidentifiers="nh_fake_speed" damagemultiplier="0.3" armorsector="0.0,0.0" />
      <sprite name="Pilot Helmet" texture="%ModDir%/Items/Jobgear/Armor/headgears.png" limb="Head" inheritlimbdepth="true" inherittexturescale="true" hidewearablesoftype="Hair,Beard" scale="1.35" sourcerect="100,200,100,100" origin="0.5,0.48">
        <LightComponent range="30" lightcolor="125,255,150,40" powerconsumption="1" IsOn="true" castshadows="false">
          <LightTexture texture="Content/Lights/lightcone_old.png" origin="0.03,0.5" size="0.1,0.1" />
        </LightComponent>
      </sprite>
      <damagemodifier afflictiontypes="damage" armorsector="0.0,360.0" damagemultiplier="0.25" damagesound="LimbArmor" />
      <damagemodifier afflictiontypes="bleeding" armorsector="0.0,360.0" damagemultiplier="0.1" damagesound="LimbArmor" />
      <damagemodifier afflictiontypes="burn" armorsector="0.0,360.0" damagemultiplier="0.5" damagesound="LimbArmor" />
      <damagemodifier afflictiontypes="stun" armorsector="0.0,360.0" damagemultiplier="0.2" damagesound="LimbArmor" />
      <damagemodifier afflictionidentifiers="concussion" armorsector="0.0,360.0" damagemultiplier="0.0" />
      <StatValue stattype="WalkingSpeed" value="-0.1" />
      <StatValue stattype="SwimmingSpeed" value="-0.7" />
      <StatusEffect type="OnWearing" targettype="This" drawhudwhenequipped="false" setvalue="true" targetitemcomponent="StatusHUD">
        <Conditional HasStatusTag="! nh_powered" />
      </StatusEffect>
    </Wearable>
    <Holdable slots="LeftHand" holdpos="15,-20" handle1="15,-5" holdangle="-10" msg="ItemMsgPickUpSelect">
      <StatusEffect type="OnActive" targettype="This" drawhudwhenequipped="false" setvalue="true" targetitemcomponent="StatusHUD" />
    </Holdable>
    <WifiComponent range="15000.0" LinkToChat="true" MinChatMessageInterval="0.0">
      <StatusEffect type="OnActive" targettype="This">
        <Conditional JamTimer="gt 0" targetitemcomponent="WifiComponent" />
        <RequiredItems items="mobilebattery" type="Contained" />
        <sound file="Content/Sounds/RadioStatic.ogg" range="500.0" frequencymultiplier="0.7" loop="true" volume="0.5" />
      </StatusEffect>
    </WifiComponent>
    <ItemContainer capacity="1" maxstacksize="1" hideitems="true" containedstateindicatorstyle="battery">
      <SlotIcon slotindex="0" texture="Content/UI/StatusMonitorUI.png" sourcerect="128,448,64,64" origin="0.5,0.5" />
      <Containable items="mobilebattery">
        <StatusEffect type="OnWearing" targettype="this" voltage="1" setvalue="true" />
        <StatusEffect type="OnWearing" targettype="Character,Contained" Condition="-0.1">
          <Conditional IsPlayer="true" />
        </StatusEffect>
        <StatusEffect type="OnWearing" targettype="Character,This" drawhudwhenequipped="true" setvalue="true" targetitemcomponent="StatusHUD" tags="nh_powered" duration="0.5">
          <Conditional InWater="false" />
        </StatusEffect>
      </Containable>
    </ItemContainer>
    <StatusHUD drawhudwhenequipped="false" overlaycolor="176,0,0,25" range="9999" thermalgoggles="true" showdeadcharacters="false" showtexts="false" />
  </Item>
